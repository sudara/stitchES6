{
  "name": "stitches6",
  "version": "1.0.0",
  "description": "Stitching Together MP3 Playback in HTML5 Audio with ES6",
  "main": "index.js",
  "scripts": {
    "start": "http-server -s c-1 . & ",
    "test": "npm start && nightwatch -c nightwatch-local.conf.json; npm stop",
    "test:single": "nightwatch -c nightwatch-local.conf.json",
    "stop": "pkill -f http-server",
    "browserstack": "npm start && node browserstack.runner.js --suiteRetries 1 --retries 1 -c nightwatch-browserstack.conf.js -e safari,chrome,chromeMac,firefox,iphoneXS,iphone11,pixel,galaxys9,ucbrowser; npm stop"
  },
  "repository": "git+https://github.com/sudara/stitches6.git",
  "keywords": [
    "mp3",
    "html5",
    "audio",
    "playback",
    "es6"
  ],
  "author": "Sudara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sudara/stitches6/issues"
  },
  "homepage": "https://github.com/sudara/stitches6#readme",
  "private": false,
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "browserstack-local": "1.4.5",
    "chromedriver": "80.0.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.1",
    "http-server": "0.12.1",
    "nightwatch": "1.3.4",
    "prettier": "1.19.1",
    "request": "2.88.2"
  },
  "prettier": {
    "semi": false
  }
}
